{
  "env": {
    "browser": true,
    "mocha": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": [1, {
      "objects": "always",
      "arrays": "ignore",
      "imports": "ignore",
      "exports": "ignore",
      "functions": "ignore"
  }]
  }
}
