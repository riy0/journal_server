{
  "name": "journal_server",
  "version": "0.1.0",
  "main": "index.js",
  "engines": {
    "node": "12.11.1"
  },
  "scripts": {
    "build": "npm run lintfix && babel src -d dist",
    "cover": "babel-node ./node_modules/istanbul/lib/cli cover --hook-run-in-context node_modules/mocha/bin/_mocha -- -R spec --timeout 10000 test/*.js",
    "coveralls": "npm run cover -- --report lcovonly && ./coverage/lcov.info > ./node_modules/coveralls/bin/coveralls.js",
    "dev": "nodemon ./dist/server",
    "lintfix": "eslint . --fix",
    "migrate": "node ./dist/migration/database.js",
    "pretest": "eslint .",
    "start": "node ./dist/server",
    "test": "babel-node node_modules/mocha/bin/_mocha test/*.js"
  },
  "author": "riy0",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validation": "^1.0.3",
    "joi": "^14.3.1",
    "morgan": "^1.9.1",
    "pg": "^7.18.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "istanbul": "^0.4.5",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2"
  }
}
